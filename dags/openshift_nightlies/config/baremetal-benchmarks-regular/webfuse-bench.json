{
    "benchmarks": [
        {
            "name": "node-density",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density",
                "PODS_PER_NODE": "245",
                "NODE_COUNT": "120",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "node-density-cni",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density-cni",
                "PODS_PER_NODE": "45",
                "NODE_COUNT": "120",
                "JOB_TIMEOUT": "18000",
                "QPS": "5",
                "BURST": "5",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "node-density-cni-networkpolicy",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density-cni-networkpolicy",
                "PODS_PER_NODE": "45",
                "NODE_COUNT": "120",
                "JOB_TIMEOUT": "18000",
                "QPS": "5",
                "BURST": "5",
                "STEP_SIZE": "30s",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "cluster-density",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "1000",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "load-cluster-preupgrade",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "1000",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "LOG_LEVEL": "info",
                "LOG_STREAMING": "true",
                "CLEANUP_WHEN_FINISH": "false",
                "CLEANUP": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "upgrades",
            "workload": "upgrade-perf",
            "trigger_rule": "all_done",
            "command": "./run_upgrade_fromgit.sh",
            "env": {
                "LATEST": "true",
                "CHANNEL": "nightlies",
                "TIMEOUT": "400",
                "POLL_INTERVAL": "10"
            }
        },
        {
            "name": "host-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "hostnet",
                "PAIRS": "1"
            }
        },
        {
            "name": "pod-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2pod"
            }
        },
        {
            "name": "serviceip-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2svc"
            }
        },
        {
            "name": "router",
            "workload": "router-perf-v2",
            "command": "./ingress-performance.sh",
            "env": {
                "THROUGHPUT_TOLERANCE": "10",
                "LATENCY_TOLERANCE": "10",
                "LARGE_SCALE_THRESHOLD": "19",
                "ENGINE": "local",
                "LARGE_SCALE_ROUTES": "400",
                "COMPARE": "false",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "GSHEET_KEY_LOCATION": "/tmp/key.json"
            }
        }
    ]
}
